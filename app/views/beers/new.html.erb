<h1>Add a new beer!</h1>
<%= form_for @beers do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :beer_type %>
  <%= f.text_field :beer_type %>
  <%= f.label :abv, "ABV%" %>
  <%= f.text_field :abv %>
  <%= f.label :ibu, "IBU" %>
  <%= f.text_field :ibu %>
  <%= f.label :image, "ImageURL" %>
  <%= f.text_field :image %>
  <%= f.submit "Add Beer" %>

<% end %>
